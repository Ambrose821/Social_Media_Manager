    <script>
        /* Declare the FAVE settings specific to this view */
        FAVE.settings.server.customer = 'cnn';
        FAVE.settings.server.edition = 'domestic';
        FAVE.settings.server.environment = 'prod\';
        FAVE.settings.mediaPlayer = 'top';

        if (typeof '' === 'string' && ''.length > 0 &&
            '' !== 'dev' && '' !== 'prod') {
            FAVE.settings.freewheel = FAVE.settings.freewheel || {};
            FAVE.settings.freewheel.serverUrl = '';
        }

        if ('' === 'true') {
            FAVE.settings.player = FAVE.settings.player || {};
            FAVE.settings.player.vr = FAVE.settings.player.vr || {};
            FAVE.settings.player.vr.grabCursorTimeout = 100;
        }

        if (typeof '' === 'string' && ''.length > 0) {
            FAVE.settings.ads = FAVE.settings.ads || {};
            FAVE.settings.ads.policies = FAVE.settings.ads.policies || {};
            FAVE.settings.ads.policies.consent = FAVE.settings.ads.policies.consent || {};
            FAVE.settings.ads.policies.consent.ccpaConsentString = FAVE.settings.ads.policies.consent.ccpaConsentString || {};
            FAVE.settings.ads.policies.consent.ccpaConsentString.defaultValue = '';
        }
