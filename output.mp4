</head>

<body>
    <div id="video" class="fave-responsive-video"></div>
    <script>
        var playerConfigs = {
                adsection: '' || COMMON.getAdsection('domestic'),
                autostart: COMMON.getAutostart(''),
                markupId: 'video',
                player: {
                    // Disable fallback to MP4 file when in a background browser tab
                    useMp4ForHiddenPages: false
                },
                server: {
                    medium: {
                        enabled: true
                    }
                },
                video: 'med093c6c63e8f8cd0eb6edd1b1d12037e53811d30',
                stellarUri: 'cms.cnn.com/_components/video-resource/instances/clyzrxlng00003b6khg8ndmf0@published',
                stellarUdk: 'rn0723c0'
            };

        if ('' === 'true') {
            playerConfigs.player.autoplay = {
                muted: {
                    enabled: true
                }
            };
        }
        if ('' === 'true') {
            playerConfigs.isVr = true;
            playerConfigs.player.maxBitrate = 'Infinity';
            playerConfigs.theoplayer = playerConfigs.theoplayer || {};
            playerConfigs.theoplayer.initialRendition = 'high';
        }
        if ('' === 'true') {
            playerConfigs.isLive = true;
            playerConfigs.player.poster = playerConfigs.player.poster || {};
            playerConfigs.player.poster.big = 'bp';
            playerConfigs.player.poster.small = 'bp';
        }
        FAVE.player({
            configs: playerConfigs
        });
    </script>
